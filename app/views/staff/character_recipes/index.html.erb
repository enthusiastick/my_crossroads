<div class="row">
<h1><%= @character.name %> Recipes Known</h1>
<% @recipes.each do |recipe| %>
<div>
<%= link_to recipe.name ,recipe_path(recipe) %>
</div>
<% end %>
</div>

<div class="row">
<%= link_to "Assign Recipe to Character", new_staff_user_character_character_recipe_path(@character.user, @character), class:"button" %>
</div>
