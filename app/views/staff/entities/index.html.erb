<div class="row">
<h1>Entities</h1>
<div class="row">
<% @entities.each do |entity|%>
<div class ="row entity">
  <%=link_to entity.name, staff_entity_path(entity) %>
  <ul class = "row">Members
    <% entity.characters.each do |character| %>
    <li class="entityMembers"><%= character.name %> </li>
    <% end %>
  </ul>

</div>
  <% end %>
</div>
<div class="row">
  <%= link_to "Create New Entity",new_staff_entity_path, class:"button" %>
</div>
</div>
